{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "dom"],
    "outDir": "dist",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": false,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "importHelpers": false,
    "downlevelIteration": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
