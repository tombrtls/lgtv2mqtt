{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "outDir": "dist",
    "module": "commonjs",
    "lib": ["es5", "dom"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": false,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "importHelpers": false,
    "downlevelIteration": false
  }
}
